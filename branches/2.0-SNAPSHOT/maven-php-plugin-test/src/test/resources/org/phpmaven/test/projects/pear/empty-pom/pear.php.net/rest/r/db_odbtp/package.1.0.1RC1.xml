<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>DB_odbtp</name>
  <summary>DB interface for ODBTP</summary>
  <description>DB_odbtp is a PEAR DB driver that uses the ODBTP extension to connect to a database.
It can be used to remotely access any Win32-ODBC accessible database from any platform.</description>
  <maintainers>
    <maintainer>
      <user>rtwitty</user>
      <name>Robert Twitty</name>
      <email>rtwitty@users.sourceforge.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.0.1RC1</version>
    <date>2004-09-29</date>
    <license>PHP</license>
    <state>beta</state>
    <notes>First release.</notes>
    <deps>
      <dep type="ext" rel="has">odbtp</dep>
      <dep type="pkg" rel="has">DB</dep>
    </deps>
    <filelist>
      <file role="php" baseinstalldir="DB" name="odbtp.php"/>
      <file role="doc" baseinstalldir="DB" name="docs/DB_odbtp.html"/>
      <file role="doc" baseinstalldir="DB" name="examples/northwind-access.php"/>
      <file role="doc" baseinstalldir="DB" name="examples/northwind-mssql.php"/>
    </filelist>
  </release>
</package>
