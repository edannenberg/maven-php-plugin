<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>RDF_RDQL</name>
  <summary>Port of the RAP RDQL API</summary>
  <description>This package is a port of the RDQL part of the RDF API for PHP (aka RAP):
http://www.wiwiss.fu-berlin.de/suhl/bizer/rdfapi/.</description>
  <maintainers>
    <maintainer>
      <user>lsmith</user>
      <name>Lukas Kahwe Smith</name>
      <email>smith@backendmedia.com</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>davey</user>
      <name>Davey Shafik</name>
      <email>davey@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1.0alpha1</version>
    <date>2004-09-21</date>
    <license>LGPL</license>
    <state>alpha</state>
    <notes>- initial release</notes>
    <deps>
      <dep type="php" rel="ge" version="4.2.0" optional="no"/>
      <dep type="pkg" rel="ge" version="1.0b1" optional="no">PEAR</dep>
      <dep type="pkg" rel="has" optional="yes">MDB</dep>
      <dep type="pkg" rel="has" optional="no">RDF</dep>
    </deps>
    <filelist>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/custom_rdql_test.php"/>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/phpdoc.css"/>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/phpdoc.dtd"/>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/php_logo.gif"/>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/rdf_metadata_button.gif"/>
      <file role="doc" baseinstalldir="/RDF" name="docs/examples/test_RdqlQuery.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL/Engine.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL/Parser.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL/ResultIterator.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL/Engine/MDB.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL/Engine/Memory.php"/>
      <file role="php" baseinstalldir="/RDF" name="RDQL.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.1.0alpha1</version>
      <date>2004-09-21</date>
      <license>LGPL</license>
      <state>alpha</state>
      <notes>- initial release
</notes>
    </release>
  </changelog>
</package>
