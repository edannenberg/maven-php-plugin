<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>File_PDF</name>
  <summary>PDF generation using only PHP.</summary>
  <description>This package provides PDF generation using only PHP, without requiring any external libraries.</description>
  <maintainers>
    <maintainer>
      <user>mdjukic</user>
      <name>Marko Djukic</name>
      <email>mdjukic@horde.org</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.0.1</version>
    <date>2003-11-01</date>
    <license>LGPL</license>
    <state>beta</state>
    <notes>Initial release as a PEAR package</notes>
    <deps>
      <dep type="php" rel="ge" version="4.2.0"/>
      <dep type="pkg" rel="has" optional="yes">HTTP_Download</dep>
    </deps>
    <provides type="class" name="PDF" />
    <filelist>
      <file role="php" baseinstalldir="/File" name="PDF.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/courier.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/helvetica.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/helveticab.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/helveticabi.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/helveticai.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/symbol.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/times.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/timesb.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/timesbi.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/timesi.php"/>
      <file role="php" baseinstalldir="/File" name="PDF/fonts/zapfdingbats.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.0.1</version>
      <date>2003-11-01</date>
      <state>beta</state>
      <notes>Initial release as a PEAR package
</notes>
    </release>
  </changelog>
</package>
