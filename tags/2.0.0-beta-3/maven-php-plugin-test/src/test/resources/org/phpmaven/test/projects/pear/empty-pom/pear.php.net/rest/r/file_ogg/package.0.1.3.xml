<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>File_Ogg</name>
  <summary>Access Ogg bitstreams.</summary>
  <description>This package provides access to various media types inside an Ogg bitsream.</description>
  <maintainers>
    <maintainer>
      <user>djg</user>
      <name>David Jonathan Grant</name>
      <email>david@grant.org.uk</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>neufeind</user>
      <name>Stefan Neufeind</name>
      <email>stefan@neufeind.net</email>
      <role>helper</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.1.3</version>
    <date>2005-09-21</date>
    <license>PHP License</license>
    <state>alpha</state>
    <notes>Package.xml changes.</notes>
    <deps>
      <dep type="php" rel="ge" version="4.1.0" optional="no"/>
    </deps>
    <filelist>
      <file role="php" name="File/Ogg.php"/>
      <file role="php" name="File/Ogg/PEAR.php"/>
      <file role="php" name="File/Ogg/Vorbis/PEAR.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.1.3</version>
      <date>2005-09-21</date>
      <state>alpha</state>
      <notes>Change to package.xml.
</notes>
    </release>
    <release>
      <version>0.1.2</version>
      <date>2003-09-09</date>
      <state>alpha</state>
      <notes>Extensive documentation for the majority of PHP elements.
</notes>
    </release>
    <release>
      <version>0.1.1</version>
      <date>2003-09-02</date>
      <state>alpha</state>
      <notes>Bugfix release:  package was installing in the wrong location.
</notes>
    </release>
    <release>
      <version>0.1</version>
      <date>2003-09-02</date>
      <state>alpha</state>
      <notes>The first release!
</notes>
    </release>
  </changelog>
</package>
