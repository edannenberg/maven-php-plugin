<?xml version="1.0" encoding="ISO-8859-1" ?>
<package version="1.0">
  <name>File_Passwd</name>
  <summary>Manipulate password files</summary>
  <description>Provides methods to manipulate standard UNIX passwd, .htpasswd
and CVS pserver passwd files.</description>
  <maintainers>
    <maintainer>
      <user>rasmus</user>
      <name>Rasmus Lerdorf</name>
      <email>rasmus@php.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>tuupola</user>
      <name>Mika Tuupola</name>
      <email>tuupola@appelsiini.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.9.1</version>
    <date>2003-08-18</date>
    <license>PHP</license>
    <state>beta</state>
    <notes>Fix for bug #24895 (Suppress a warning if the passwd file 
does not have 3 columns )</notes>
    <provides type="class" name="File_Passwd" />
    <provides type="function" name="File_Passwd::addUser" />
    <provides type="function" name="File_Passwd::modUser" />
    <provides type="function" name="File_Passwd::delUser" />
    <provides type="function" name="File_Passwd::verifyPassword" />
    <provides type="function" name="File_Passwd::listUsers" />
    <provides type="function" name="File_Passwd::close" />
    <provides type="function" name="File_Passwd::lock" />
    <provides type="function" name="File_Passwd::unlock" />
    <provides type="function" name="File_Passwd::isLocked" />
    <provides type="function" name="File_Passwd::getCvsUser" />
    <filelist>
      <file role="php" baseinstalldir="File" md5sum="889916117e0cf63283ade2d46803585e" name="Passwd.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>0.9.0</version>
      <date>2002-10-30</date>
      <notes>Initial release of the original version. The 1.0.0 version will
have some adjustments which will break backwards compatibility.
</notes>
    </release>
  </changelog>
</package>
