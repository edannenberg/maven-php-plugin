      <file role="php" baseinstalldir="I18Nv2" name="Locale\ru.php"/>
      <file role="php" baseinstalldir="I18Nv2" name="Locale\sl.php"/>
      <file role="php" baseinstalldir="I18Nv2" name="Locale\Map.php"/>
      <file role="php" baseinstalldir="I18Nv2" name="Locale\MapWindows.php"/>
