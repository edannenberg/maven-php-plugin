<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>Net_DNSBL</name>
  <summary>DNSBL Checker</summary>
  <description>Checks if a given Host or URL is listed on an DNSBL or SURBL</description>
  <maintainers>
    <maintainer>
      <user>nohn</user>
      <name>Sebastian Nohn</name>
      <email>sebastian@nohn.net</email>
      <role>lead</role>
    </maintainer>
    <maintainer>
      <user>ammarmi</user>
      <name>Ammar Ibrahim</name>
      <email>ammar@gnuix.com</email>
      <role>developer</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.5.1</version>
    <date>2004-11-28</date>
    <license>PHP License</license>
    <state>beta</state>
    <notes>* Fixed coding standards issues
           * Made beta as the package is documented and proven to work but the guidelines don't allow to make the first release stable</notes>
    <deps>
      <dep type="pkg" rel="ge" version="1.5.4">Cache</dep>
      <dep type="pkg" rel="ge" version="1.1">Net_CheckIP</dep>
      <dep type="pkg" rel="ge" version="1.2.3">HTTP_Request</dep>
      <dep type="php" rel="ge" version="4.3.6"/>
    </deps>
    <filelist>
      <file role="php" baseinstalldir="Net" name="DNSBL.php"/>
      <file role="php" baseinstalldir="Net" name="DNSBL\SURBL.php"/>
      <file role="test" baseinstalldir="Net" name="Tests\testNetDNSBL.php"/>
      <file role="test" baseinstalldir="Net" name="Tests\testNetDNSBLSURBL.php"/>
    </filelist>
  </release>
</package>
