   <role>lead</role>
  </maintainer>
  </maintainers>
 <release>
  <version>0.7.0</version>
  <date>2006-03-31</date>
  <license>BSD</license>
  <state>beta</state>
  <notes>Initial release
  </notes>
  <deps>
   <dep type="pkg" rel="ge" version="1.3.3" optional="no">PEAR</dep>
  </deps>
