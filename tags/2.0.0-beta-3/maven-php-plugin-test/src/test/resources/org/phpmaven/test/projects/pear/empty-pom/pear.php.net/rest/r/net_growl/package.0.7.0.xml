  <provides type="function" name="Net_Growl_Application::addGrowlNotifications" />
  <provides type="function" name="Net_Growl_Application::getGrowlNotifications" />
  <provides type="function" name="Net_Growl_Application::getGrowlName" />
  <provides type="function" name="Net_Growl_Application::getGrowlPassword" />
  <provides type="class" name="Net_Growl" extends="PEAR" />
  <provides type="function" name="Net_Growl::singleton" />
  <provides type="function" name="Net_Growl::setNotificationLimit" />
  <provides type="function" name="Net_Growl::getApplication" />
