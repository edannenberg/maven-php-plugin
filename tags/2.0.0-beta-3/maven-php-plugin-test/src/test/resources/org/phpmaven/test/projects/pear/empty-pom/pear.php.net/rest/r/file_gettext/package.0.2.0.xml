<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>File_Gettext</name>
  <summary>GNU Gettext file parser</summary>
  <description>Reader and writer for GNU PO and MO files.</description>
  <maintainers>
    <maintainer>
      <user>mike</user>
      <name>Michael Wallner</name>
      <email>mike@php.net</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>0.2.0</version>
    <date>2004-03-30</date>
    <license>PHP</license>
    <state>alpha</state>
    <notes>* Initial PEAR release</notes>
    <deps>
      <dep type="pkg" rel="has" optional="no">PEAR</dep>
      <dep type="ext" rel="has" optional="no">pcre</dep>
      <dep type="php" rel="ge" version="4.1.0" optional="no"/>
    </deps>
    <filelist>
      <file role="php" baseinstalldir="File" name="Gettext.php"/>
      <file role="php" baseinstalldir="File" name="Gettext\MO.php"/>
      <file role="php" baseinstalldir="File" name="Gettext\PO.php"/>
    </filelist>
  </release>
</package>
